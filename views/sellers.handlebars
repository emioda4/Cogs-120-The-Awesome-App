<body>

<div class="container1">
	<hr>
    <h2><b>Sellers</b></h2>
	</div>  

<hr>
{{#each users}}
    <div class="container2" class="users">
        <a href = "users" class = "thumbnail">
        <table style="width:100%">
            <tr>
                <th>
                	<img src="{{this.imageURL}}">
                </th>
                <th>
                    <p>{{name}}</p>
                    <h3>{{amount}}</h3>
                </th> 
                <th>
                    <input type="button" id="sendButton" value="Send Request to Buy From" onclick="window.location.href='requestReceived2'" />
                </th>

            </tr>
        </table>
    </div> 
{{/each}}
